{
    "name": "eslint-plugin-react-ts",
    "version": "0.2.2",
    "description": "ESLint plugin to lint TypeScript React and Preact function components using the @typescript-eslint ecosystem.",
    "keywords": [
        "eslint",
        "eslint-plugin",
        "eslint-plugin-react-ts",
        "react",
        "react-ts",
        "react-tsx",
        "ts",
        "tsx",
        "typescript",
        "typescript-eslint",
        "typescript-react"
    ],
    "homepage": "https://github.com/Rel1cx/eslint-plugin-react-ts",
    "bugs": {
        "url": "https://github.com/Rel1cx/eslint-plugin-react-ts/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Rel1cx/eslint-plugin-react-ts.git"
    },
    "license": "MIT",
    "author": "Eva1ent<let@ik.me>",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./configs/all": {
            "require": "./dist/configs/all.js",
            "import": "./dist/configs/all.mjs"
        },
        "./configs/debug": {
            "require": "./dist/configs/debug.js",
            "import": "./dist/configs/debug.mjs"
        },
        "./configs/recommended": {
            "require": "./dist/configs/recommended.js",
            "import": "./dist/configs/recommended.mjs"
        },
        "./configs/recommended-type-checked": {
            "require": "./dist/configs/recommended-type-checked.js",
            "import": "./dist/configs/recommended-type-checked.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "files": [
        "./dist",
        "./package.json"
    ],
    "scripts": {
        "build": "rollup -c rollup.config.ts --configPlugin swc3",
        "docs:generate": "typedoc && bun run ./scripts/doc-generate.ts",
        "fix:ts": "eslint --ext .ts --fix .",
        "format:check": "dprint check",
        "format:write": "dprint fmt",
        "lint": "bun run format:check && bun run lint:spell && bun run lint:deps && bun run lint:ts && bun run lint:type",
        "lint:deps": "skott -m file-tree -e .ts -s",
        "lint:spell": "cspell lint --relative --no-progress '**'",
        "lint:ts": "eslint --cache --ext .ts .",
        "lint:type": "tsc --noEmit",
        "prepare": "husky install",
        "publish": "bun run lint && bun run build && bun run test && bun run publint",
        "test": "vitest --run --single-thread --no-threads"
    },
    "dependencies": {
        "@typescript-eslint/scope-manager": ">=6.7.3",
        "@typescript-eslint/type-utils": ">=6.7.3",
        "@typescript-eslint/types": ">=6.7.3",
        "@typescript-eslint/utils": ">=6.7.3"
    },
    "devDependencies": {
        "@effect/data": "0.18.7",
        "@effect/language-service": "0.0.21",
        "@rizzzse/bimap": "1.2.1",
        "@rollup/plugin-commonjs": "25.0.4",
        "@rollup/plugin-json": "6.0.0",
        "@rollup/plugin-node-resolve": "15.2.1",
        "@swc/core": "1.3.91",
        "@tsconfig/bun": "1.0.1",
        "@tsconfig/strictest": "2.0.2",
        "@typescript-eslint/eslint-plugin": "6.7.3",
        "@typescript-eslint/rule-tester": "6.7.3",
        "birecord": "0.1.1",
        "bun-types": "1.0.3",
        "cspell": "7.3.7",
        "dedent": "1.5.1",
        "dprint": "0.41.0",
        "eslint": ">=8.50.0",
        "eslint-config-with-tsconfig": "2.7.0",
        "eslint-plugin-eslint-plugin": "5.1.1",
        "eslint-plugin-filenames-simple": "0.8.0",
        "eslint-plugin-jsdoc": "46.8.2",
        "eslint-plugin-vitest": "0.3.1",
        "fast-equals": "5.0.1",
        "fast-glob": "3.3.1",
        "husky": "8.0.3",
        "micro-memoize": "4.1.2",
        "nolyfill": "1.0.21",
        "pathe": "1.1.1",
        "pattycake": "0.0.2",
        "publint": "0.2.3",
        "rambda": "8.4.0",
        "rollup": "3.29.4",
        "rollup-plugin-dts": "6.0.2",
        "rollup-plugin-swc3": "0.10.1",
        "skott": "0.31.0",
        "tiny-invariant": "1.3.1",
        "ts-pattern": "5.0.5",
        "type-fest": "4.3.3",
        "typedoc": "0.25.1",
        "typescript": ">=5.2.2",
        "vitest": "0.34.6"
    },
    "peerDependencies": {
        "@typescript-eslint/parser": ">=6.7.3",
        "eslint": ">=8.50.0",
        "typescript": ">=5.2.2"
    },
    "packageManager": "bun@1.0.3",
    "engines": {
        "bun": ">=1.0.3",
        "node": ">=18.16.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
