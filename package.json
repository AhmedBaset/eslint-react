{
    "name": "eslint-plugin-react-ts",
    "version": "0.1.4",
    "description": "ESLint plugin to lint TypeScript React function components using the @typescript-eslint ecosystem.",
    "keywords": [
        "eslint",
        "react",
        "ts",
        "tsx",
        "typescript",
        "eslint-plugin",
        "eslint-plugin-react-ts"
    ],
    "bugs": {
        "url": "https://github.com/Rel1cx/eslint-plugin-react-ts/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Rel1cx/eslint-plugin-react-ts.git"
    },
    "license": "MIT",
    "author": "Eva1ent<let@ik.me>",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./configs/all": {
            "require": "./dist/configs/all.js",
            "import": "./dist/configs/all.mjs"
        },
        "./configs/debug": {
            "require": "./dist/configs/debug.js",
            "import": "./dist/configs/debug.mjs"
        },
        "./configs/recommended": {
            "require": "./dist/configs/recommended.js",
            "import": "./dist/configs/recommended.mjs"
        },
        "./configs/recommended-type-checked": {
            "require": "./dist/configs/recommended-type-checked.js",
            "import": "./dist/configs/recommended-type-checked.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "files": [
        "./dist"
    ],
    "scripts": {
        "build": "tsup",
        "docs:generate": "bun run ./scripts/doc-generate.ts",
        "fix:ts": "eslint --ext .ts --fix .",
        "format:check": "prettier -u .",
        "format:write": "prettier -w -u .",
        "lint": "bun run lint:spell && bun run lint:deps && bun run lint:ts && bun run lint:type",
        "lint:deps": "skott -m graph -e .ts -s",
        "lint:spell": "cspell lint --relative --no-progress '**'",
        "lint:ts": "eslint --ext .ts .",
        "lint:type": "tsc --noEmit",
        "prepare": "husky install",
        "publish": "bun run format:write && bun run lint && bun run test && bun run build && bun run publint",
        "test": "vitest --run"
    },
    "prettier": "eslint-config-with-tsconfig/prettier",
    "dependencies": {
        "@effect/data": "0.18.5",
        "@typescript-eslint/parser": "6.7.2",
        "@typescript-eslint/scope-manager": "6.7.2",
        "@typescript-eslint/type-utils": "6.7.2",
        "@typescript-eslint/types": "6.7.2",
        "@typescript-eslint/utils": "6.7.2",
        "ts-pattern": "5.0.5"
    },
    "devDependencies": {
        "@effect/language-service": "0.0.21",
        "@tsconfig/strictest": "2.0.2",
        "@typescript-eslint/eslint-plugin": "6.7.2",
        "@typescript-eslint/rule-tester": "6.7.2",
        "birecord": "0.1.1",
        "bun-types": "1.0.2",
        "cspell": "7.3.6",
        "eslint": "8.49.0",
        "eslint-config-relicx": "0.12.6",
        "eslint-config-with-tsconfig": "2.6.7",
        "eslint-doc-generator": "1.4.3",
        "eslint-plugin-eslint-plugin": "5.1.1",
        "eslint-plugin-filenames-simple": "0.8.0",
        "fast-equals": "5.0.1",
        "fast-glob": "3.3.1",
        "husky": "8.0.3",
        "micro-memoize": "4.1.2",
        "nolyfill": "1.0.21",
        "pathe": "1.1.1",
        "prettier": "3.0.3",
        "publint": "0.2.2",
        "skott": "0.29.0",
        "tiny-invariant": "1.3.1",
        "tsup": "7.2.0",
        "type-fest": "4.3.1",
        "typescript": "5.2.2",
        "vitest": "0.34.4",
        "zx": "7.2.3"
    },
    "peerDependencies": {
        "eslint": ">=8.49.0",
        "typescript": ">=5.2.2"
    },
    "packageManager": "bun@1.0.2",
    "engines": {
        "bun": ">=1.0.2",
        "node": ">=18.16.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
