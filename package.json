{
    "name": "eslint-plugin-react-ts",
    "version": "0.1.0",
    "description": "ESLint plugin to lint React TypeScript apps using the @typescript-eslint ecosystem.",
    "keywords": [
        "eslint",
        "eslint-plugin",
        "react",
        "tsx",
        "ts"
    ],
    "bugs": {
        "url": "https://github.com/Rel1cx/eslint-plugin-react-ts/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Rel1cx/eslint-plugin-react-ts.git"
    },
    "license": "MIT",
    "author": "Eva1ent<let@ik.me>",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./configs/all": {
            "require": "./dist/configs/all.js",
            "import": "./dist/configs/all.mjs"
        },
        "./configs/recommended": {
            "require": "./dist/configs/recommended.js",
            "import": "./dist/configs/recommended.mjs"
        },
        "./configs/recommended-type-checked": {
            "require": "./dist/configs/recommended-type-checked.js",
            "import": "./dist/configs/recommended-type-checked.mjs"
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "files": [
        "./dist"
    ],
    "scripts": {
        "build": "tsup",
        "docs:generate": "esno ./scripts/doc-generate.ts",
        "fix:ts": "eslint --ext .ts --fix .",
        "format:check": "prettier -u .",
        "format:write": "prettier -w -u .",
        "lint:deps": "skott -m graph -e .ts -s",
        "lint:spell": "cspell lint --relative --no-progress '**'",
        "lint:ts": "eslint --ext .ts .",
        "lint:type": "tsc --noEmit",
        "prepare": "husky install",
        "publish": "pnpm format:write && pnpm run '/^lint:/' && pnpm test && pnpm build && pnpm publint",
        "test": "vitest --run --single-thread --no-threads"
    },
    "dependencies": {
        "@effect/data": "0.17.6",
        "@typescript-eslint/parser": "6.5.0",
        "@typescript-eslint/scope-manager": "6.5.0",
        "@typescript-eslint/type-utils": "6.5.0",
        "@typescript-eslint/utils": "6.5.0",
        "ts-pattern": "5.0.5"
    },
    "devDependencies": {
        "@tsconfig/strictest": "2.0.1",
        "@types/node": "20.5.7",
        "@typescript-eslint/eslint-plugin": "6.5.0",
        "@typescript-eslint/rule-tester": "6.5.0",
        "cspell": "7.2.0",
        "eslint": "8.48.0",
        "eslint-config-relicx": "0.11.3",
        "eslint-config-with-tsconfig": "2.5.2",
        "eslint-doc-generator": "1.4.3",
        "eslint-plugin-eslint-plugin": "5.1.1",
        "eslint-plugin-filenames-simple": "0.8.0",
        "esno": "0.17.0",
        "fast-glob": "3.3.1",
        "husky": "8.0.3",
        "micro-memoize": "4.1.2",
        "pathe": "1.1.1",
        "prettier": "3.0.3",
        "prettier-plugin-curly": "0.1.2",
        "prettier-plugin-organize-attributes": "1.0.0",
        "prettier-plugin-packagejson": "2.4.5",
        "publint": "0.2.2",
        "skott": "0.28.2",
        "tiny-invariant": "1.3.1",
        "tsup": "7.2.0",
        "type-fest": "4.3.1",
        "typescript": "5.2.2",
        "vitest": "0.34.3",
        "zx": "7.2.3"
    },
    "peerDependencies": {
        "eslint": ">=8.48.0",
        "typescript": ">=5.2.2"
    },
    "packageManager": "pnpm@8.7.0",
    "engines": {
        "node": ">=18.16.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "pnpm": {
        "overrides": {
            "typescript": "latest"
        }
    }
}
