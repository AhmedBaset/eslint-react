{
    "cleanOutputDir": true,
    "darkHighlightTheme": "dark-plus",
    "disableSources": true,
    "entryPoints": [
        "src/index.ts"
    ],
    "exclude": [
        "src/rule-name.ts"
    ],
    "hideGenerator": true,
    "jsDocCompatibility": true,
    "logLevel": "Warn",
    "name": "@eslint-react/types",
    "out": "docs",
    "readme": "none",
    "sort": [
        "kind",
        "instance-first",
        "alphabetical"
    ],
    "plugin": [
        "typedoc-plugin-markdown"
    ],
    "theme": "default",
    "useTsLinkResolution": true
}
