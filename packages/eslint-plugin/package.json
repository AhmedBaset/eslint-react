{
    "name": "@eslint-react/eslint-plugin",
    "version": "0.3.0",
    "description": "ESLint plugin for React and Preact function components with TypeScript, built from scratch (almost).",
    "keywords": [
        "eslint",
        "eslint-plugin",
        "eslint-react",
        "react",
        "react-ts",
        "react-tsx",
        "ts",
        "tsx",
        "typescript",
        "typescript-eslint",
        "typescript-react"
    ],
    "homepage": "https://github.com/Rel1cx/eslint-react",
    "bugs": {
        "url": "https://github.com/Rel1cx/eslint-react/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Rel1cx/eslint-react.git",
        "directory": "packages/eslint-plugin"
    },
    "license": "MIT",
    "author": "Eva1ent<let@ik.me>",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./configs/all": {
            "require": "./dist/configs/all.js",
            "import": "./dist/configs/all.mjs"
        },
        "./configs/off": {
            "require": "./dist/configs/off.js",
            "import": "./dist/configs/off.mjs"
        },
        "./configs/debug": {
            "require": "./dist/configs/debug.js",
            "import": "./dist/configs/debug.mjs"
        },
        "./configs/recommended": {
            "require": "./dist/configs/recommended.js",
            "import": "./dist/configs/recommended.mjs"
        },
        "./configs/recommended-type-checked": {
            "require": "./dist/configs/recommended-type-checked.js",
            "import": "./dist/configs/recommended-type-checked.mjs"
        },
        "./package.json": "./package.json"
    },
    "type": "commonjs",
    "main": "./dist/index.js",
    "files": [
        "./dist",
        "./package.json"
    ],
    "scripts": {
        "build": "rollup -c rollup.config.ts --configPlugin swc3",
        "build:docs-rules": "bun run ./scripts/doc-generate.ts && eslint-doc-generator && dprint fmt docs/rules/**/*.md",
        "publish": "bun run lint && bun run build && bun run test && bun x publint",
        "test": "vitest --run --single-thread --no-threads"
    },
    "dependencies": {
        "@typescript-eslint/scope-manager": "6.7.5",
        "@typescript-eslint/type-utils": "6.7.5",
        "@typescript-eslint/types": "6.7.5",
        "@typescript-eslint/utils": "6.7.5"
    },
    "devDependencies": {
        "@eslint-react/std": "workspace:*",
        "@eslint-react/ast": "workspace:*",
        "@eslint-react/component": "workspace:*",
        "@eslint-react/component-legacy": "workspace:*",
        "@eslint-react/construction": "workspace:*",
        "@eslint-react/context": "workspace:*",
        "@eslint-react/create-element": "workspace:*",
        "@eslint-react/hooks": "workspace:*",
        "@eslint-react/jsx": "workspace:*",
        "@eslint-react/pragma": "workspace:*",
        "@eslint-react/render-prop": "workspace:*",
        "@eslint-react/shared": "workspace:*",
        "@eslint-react/variable": "workspace:*",
        "@rizzzse/bimap": "1.2.1",
        "@typescript-eslint/rule-tester": "6.7.5",
        "birecord": "0.1.1",
        "dedent": "1.5.1",
        "effect": "2.0.0-next.48",
        "fast-equals": "5.0.1",
        "micro-memoize": "4.1.2",
        "rambda": "8.5.0",
        "string-ts": "1.3.0",
        "ts-pattern": "5.0.5",
        "vitest": "0.34.6"
    },
    "peerDependencies": {
        "@typescript-eslint/parser": ">=6.7.5",
        "eslint": ">=8.51.0",
        "typescript": "5.2.2"
    },
    "packageManager": "bun@1.0.5",
    "engines": {
        "bun": ">=1.0.4",
        "node": ">=18.16.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
